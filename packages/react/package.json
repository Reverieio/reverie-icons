{
  "name": "@reverieio/icons-react",
  "version": "1.0.0",
  "description": "React components for the Reverie icon library",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./icons/*": {
      "import": {
        "types": "./dist/icons/*.d.mts",
        "default": "./dist/icons/*.mjs"
      },
      "require": {
        "types": "./dist/icons/*.d.cts",
        "default": "./dist/icons/*.cjs"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@reverieio/icons-core": "^1.0.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.3.2",
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  },
  "keywords": [
    "icons",
    "react",
    "svg",
    "reverie",
    "components",
    "icon-library"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Reverieio/reverie-icons",
    "directory": "packages/react"
  }
}
