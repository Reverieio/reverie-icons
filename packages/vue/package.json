{
  "name": "@reverieio/icons-vue",
  "version": "1.0.0",
  "description": "Vue 3 components for the Reverie icon library",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "files": ["dist", "README.md"],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@reverieio/icons-core": "^1.0.0"
  },
  "peerDependencies": {
    "vue": ">=3.3.0"
  },
  "devDependencies": {
    "vue": "^3.4.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  },
  "keywords": ["icons", "vue", "svg", "reverie", "icon-library"],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Reverieio/reverie-icons",
    "directory": "packages/vue"
  }
}
