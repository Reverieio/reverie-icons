{
  "name": "@reverieio/icons-core",
  "version": "1.0.0",
  "description": "Framework-agnostic SVG icon data for the Reverie icon library",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./icons/*": {
      "import": {
        "types": "./dist/icons/*.d.mts",
        "default": "./dist/icons/*.mjs"
      },
      "require": {
        "types": "./dist/icons/*.d.cts",
        "default": "./dist/icons/*.cjs"
      }
    },
    "./metadata": {
      "import": {
        "types": "./dist/metadata.d.mts",
        "default": "./dist/metadata.mjs"
      },
      "require": {
        "types": "./dist/metadata.d.cts",
        "default": "./dist/metadata.cjs"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  },
  "keywords": [
    "icons",
    "svg",
    "reverie",
    "fragrance",
    "icon-library",
    "design-system"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Reverieio/reverie-icons",
    "directory": "packages/core"
  }
}
